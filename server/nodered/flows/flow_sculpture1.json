    {
        "id": "sculpture1_mute_state",
        "type": "function",
        "z": "sculpture_dashboard",
        "name": "Mute State 1",
        "func": "// Store the mute state in a flow variable so the template can access it.\nflow.set('sculpture1_is_muted', msg.payload);\n\n// Pass the message on to the template to trigger a refresh.\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 450,
        "y": 700,
        "wires": [
            [
                "sculpture1_mute_button"
            ]
        ]
    },
    {
        "id": "sculpture1_mute_button",
        "type": "ui_template",
        "z": "sculpture_dashboard",
        "group": "ui_group_sculpture1",
        "name": "Mute/Unmute Button 1",
        "order": 7,
        "width": 0,
        "height": 0,
        "format": "<md-button \n    ng-init='isMuted = flow.get(\"sculpture1_is_muted\") || false' \n    ng-style=\"{ 'background-color': isMuted ? '#4CAF50' : '#f44336', 'color': 'white' }\"\n    ng-click=\"send({ payload: { 'mute': !isMuted } })\">\n    <i class=\"fa {{isMuted ? 'fa-volume-up' : 'fa-volume-off'}}\"></i> \n    {{isMuted ? 'Unmute' : 'Mute'}}\n</md-button>",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "templateScope": "local",
        "x": 650,
        "y": 700,
        "wires": [
            [
                "mqtt_out_1"
            ]
        ]
    },
